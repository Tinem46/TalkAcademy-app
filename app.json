{
  "expo": {
    "name": "Talkademy",
    "slug": "Talk-Academy",
    "version": "1.1.0",
    "orientation": "portrait",
    "icon": "./src/assets/Mascot/icon.png",
    "scheme": "talkacademy",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
       "foregroundImage": "./src/assets/Mascot/androidIcon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.tin.talkacademy",
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./src/assets/Mascot/Asset1logoFB.png"
    },
    "plugins": [
      "expo-router",
      "expo-image-picker",
      [
        "expo-splash-screen",
        {
          "image": "./src/assets/Mascot/Asset1logoFB.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "0b98e6d7-3aba-4d06-ab42-9afcfdee2672"
      }
    },
    "owner": "khangho46"
  }
}